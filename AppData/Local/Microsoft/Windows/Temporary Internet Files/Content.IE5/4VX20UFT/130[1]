	function ClearReceiveEvent()
	{
	    ReceiveEvent = true;
	    window.clearInterval(MyInterval);
	//    alert('come in ClearReceiveEvent');
	}
	
	
	function ChangeFileName(file)
	{
	    if (ReceiveEvent == true)
	    {
    	    MyInterval = window.setInterval("ClearReceiveEvent()", 1200);
	        ReceiveEvent = false;
 //   	    alert(file);
	    	var x=document.getElementById("learnmore1")
    		x.innerHTML = file;
	    	x=document.getElementById("txtLearnMore")
    		x.focus(); //focus is explicitly set
		    x.fireEvent("onclick", "");
//		    event.cancelBubble=true;
	    }
	}
