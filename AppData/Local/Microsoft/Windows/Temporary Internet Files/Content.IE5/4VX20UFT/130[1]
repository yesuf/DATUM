	function AllCheck()
	{
		var i, name, numToString, flag;
		
		flag = document.user.SelectAll.checked;
		for (i = 1; i < 41; i++)
		{
			name = "cbList";
		   	numToString = parseInt(i);
			name = name + numToString;
			a = document.getElementsByName(name);
			if (a.length != "0")
			{
				switch(i){
					case 1:
						document.user.cbList1.checked = flag;
						break;
						
					case 2:
						document.user.cbList2.checked = flag;
						break;
						
					case 3:
